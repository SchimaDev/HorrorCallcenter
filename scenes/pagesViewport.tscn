[sub_resource type="GDScript" id="GDScript_wjkbp"]
script/source = "extends Control

@onready var view_1 = $BookMesh2Untitled/View1
@onready var view_2 = $BookMesh2Untitled/View2
@onready var view_3 = $BookMesh2Untitled/View3
@onready var view_4 = $BookMesh2Untitled/View4
@onready var view_5 = $BookMesh2Untitled/View5
@onready var view_6 = $BookMesh2Untitled/View6

signal turnNextPage
signal turnPrevPage

func _on_rich_text_label_meta_clicked(meta):
	ClickTextEventHandler.handle_Encyclopedia_url_tag_clicked(meta)

# Called when the node enters the scene tree for the first time.
func _ready():
	view_1.visible = true
	view_2.visible = false
	view_3.visible = false
	view_4.visible = false
	view_5.visible = false
	view_6.visible = false

func showPages(p: int):
	match p:
		0, 1:
			view_1.visible = true
			view_2.visible = false
		2:
			view_1.visible = false
			view_2.visible = true
			view_3.visible = false
		3:
			view_2.visible = false
			view_3.visible = true
			view_4.visible = false 
		4:
			view_3.visible = false
			view_4.visible = true
			view_5.visible = false
		5:
			view_4.visible = false
			view_5.visible = true
			view_6.visible = false
		6,7:
			view_5.visible = false
			view_6.visible = true

func nextPage():
	turnNextPage.emit()

func prevPage():
	turnPrevPage.emit()
"

